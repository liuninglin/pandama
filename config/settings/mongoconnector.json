{
    "oplogFile": "./oplog.timestamp",
    "noDump": false,
    "batchSize": 50,
    "verbosity": 2,
    "continueOnError": true,
    "logging": {
        "type": "stream"
    },
    "namespaces": {
         "test.catalogs_catalog": {
             "rename": "catalogs_catalog._doc"
         }
     },
    "docManagers": [
        {
            "docManager": "elastic2_doc_manager",
            "targetURL": "172.17.0.4:9200",
            "bulkSize": 10,
            "uniqueKey": "_id",
            "args": {
               "clientOptions": {"timeout": 5000}
            }
        }
    ]
 }